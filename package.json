{
  "name": "vuex-workshop",
  "version": "1.0.0",
  "description": "A Vuex workshop",
  "author": "Blake Newman <blake.newman@askattest.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development node server",
    "start": "cross-env NODE_ENV=production node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client":
      "cross-env NODE_ENV=production webpack --config config/webpack.client.js --progress --hide-modules",
    "build:server":
      "cross-env NODE_ENV=production webpack --config config/webpack.ssr.js --progress --hide-modules",
    "lint": "yarn lint:prettier --write && yarn lint:eslint --fix && yarn lint:stylelint --fix",
    "lint:prettier": "prettier --ignore-path .gitignore './**/*.+(js|jsx|json|vue)'",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .json,.js,.jsx,.vue .",
    "lint:stylelint":
      "stylelint 'src/**/*.@(css|vue)' --custom-syntax postcss-html --config .stylelintrc.js"
  },
  "engines": {
    "node": ">=8.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "axios": "0.18.0",
    "babel-polyfill": "6.26.0",
    "normalize.css": "8.0.0",
    "reset.css": "2.0.2",
    "vue": "2.5.16",
    "vue-router": "3.0.1",
    "vuex": "3.0.1",
    "vuex-router-sync": "5.0.0"
  },
  "devDependencies": {
    "@vue/test-utils": "1.0.0-beta.12",
    "autoprefixer": "8.2.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-helper-vue-jsx-merge-props": "2.0.3",
    "babel-loader": "7.1.4",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-transform-new-target": "7.0.0-alpha.14",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-plugin-transform-vue-jsx": "3.7.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-2": "6.24.1",
    "body-parser": "1.18.2",
    "cache-loader": "1.2.2",
    "compression": "1.7.2",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-standard": "3.0.1",
    "eslint-plugin-vue": "4.4.0",
    "express": "4.16.3",
    "file-loader": "1.1.11",
    "friendly-errors-webpack-plugin": "1.6.1",
    "get-port": "3.2.0",
    "launch-editor-middleware": "2.2.1",
    "lru-cache": "4.1.2",
    "memory-fs": "0.4.1",
    "postcss-html": "0.17.0",
    "rimraf": "2.6.2",
    "source-map-support": "0.5.4",
    "spdy": "3.4.7",
    "stylelint": "9.1.3",
    "stylelint-config-idiomatic-order": "5.0.0",
    "stylelint-config-prettier": "3.0.4",
    "stylelint-config-standard": "18.2.0",
    "stylelint-processor-html": "1.0.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "thread-loader": "1.1.5",
    "url-loader": "1.0.1",
    "vue-loader": "14.2.1",
    "vue-server-renderer": "2.5.16",
    "vue-style-loader": "4.1.0",
    "vue-template-compiler": "2.5.16",
    "webpack": "4.2.0",
    "webpack-cli": "2.0.13",
    "webpack-dev-middleware": "3.0.1",
    "webpack-hot-middleware": "2.21.2",
    "webpack-merge": "4.1.2",
    "webpack-node-externals": "1.6.0"
  }
}
